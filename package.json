{
  "name": "@atomist/atomist-client-sdm",
  "version": "0.1.0",
  "description": "Atomist Software Delivery Machine (SDM) that delivers Atomist open-source projects",
  "author": {
    "name": "Atomist",
    "email": "support@atomist.com",
    "url": "https://atomist.com/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/atomist/atomist-client-sdm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/atomist-client-sdm.git"
  },
  "bugs": {
    "url": "https://github.com/atomist/atomist-client-sdm/issues"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/automation-client": "^1.8.0-master.20191004160448",
    "@atomist/automation-client-ext-dashboard": "^1.1.0",
    "@atomist/automation-client-ext-humio": "^0.2.1",
    "@atomist/automation-client-ext-raven": "^1.0.3",
    "@atomist/sdm": "^1.8.0-master.20190926164141",
    "@atomist/sdm-core": "^1.8.0-master.20191004141835",
    "@atomist/sdm-pack-build": "^1.0.7-master.20190916030741",
    "@atomist/sdm-pack-changelog": "^1.0.3",
    "@atomist/sdm-pack-docker": "^2.0.3-master.20191004184845",
    "@atomist/sdm-pack-fingerprint": "^5.0.1-master.20191003103359",
    "@atomist/sdm-pack-gcp": "0.1.0-master.20191002152051",
    "@atomist/sdm-pack-k8s": "^1.10.2",
    "@atomist/sdm-pack-node": "^1.2.0-master.20190921230202",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@atomist/sdm-local": "^1.2.1",
    "@types/lodash": "^4.14.141",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0",
    "supervisor": "^0.12.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js}{,.map}\" \"{lib,test}/**/*.{d.ts,js}{,.map}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --config tslint.json --format verbose --project .",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.0.0"
  }
}
